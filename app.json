{
  "expo": {
    "name": "EuReciclo",
    "slug": "EuReciclo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/inter/Poppins_400Regular.ttf",
            "node_modules/@expo-google-fonts/inter/Poppins_500Medium.ttf",
            "node_modules/@expo-google-fonts/inter/Poppins_700Bold.ttf"
         ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Aceita que $(PRODUCT_NAME) acesse sua c√¢mera",
          "microphonePermission": "Aceita que $(PRODUCT_NAME) acesse seu microfone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "O Aplicativo deseja acesso a sua galeria"
        }
      ]
    ]
  }
}
